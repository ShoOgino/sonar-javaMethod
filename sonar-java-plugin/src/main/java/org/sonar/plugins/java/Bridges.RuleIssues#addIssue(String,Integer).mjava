    public void addIssue(String name, Integer line) {
      FileIssues fileIssues = files.get(name);
      if (fileIssues == null) {
        fileIssues = new FileIssues();
        files.put(name, fileIssues);
      }
      if(line == null) {
        line = 0;
      }
      fileIssues.lines.add(line);
    }

