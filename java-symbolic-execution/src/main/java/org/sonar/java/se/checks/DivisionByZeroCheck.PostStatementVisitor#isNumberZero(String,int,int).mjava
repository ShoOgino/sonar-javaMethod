    private static boolean isNumberZero(String literalValue, int startIndex, int endIndex) {
      for (int i = startIndex; i <= endIndex; ++i) {
        if (literalValue.charAt(i) != '_' && literalValue.charAt(i) != '0' && literalValue.charAt(i) != '.') {
          return false;
        }
      }
      return true;
    }

