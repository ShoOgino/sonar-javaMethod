  public boolean executeCheckPreStatement(Tree syntaxNode) {
    this.syntaxNode = syntaxNode;
    ProgramState ps;
    for (SECheck checker : checks) {
      ps = checker.checkPreStatement(this, syntaxNode);
      if(ps == null) {
        return false;
      }
      explodedGraphWalker.programState = ps;
    }
    return true;

  }

