  private void createParentLink(JavaTree tree) {
    if (!tree.isLeaf()) {
      for (Iterator<Tree> iter = tree.childrenIterator(); iter.hasNext(); ) {
        Tree next = iter.next();
        if (next != null) {
          parentLink.put(next, tree);
          createParentLink((JavaTree) next);
        }
      }
    }
  }

