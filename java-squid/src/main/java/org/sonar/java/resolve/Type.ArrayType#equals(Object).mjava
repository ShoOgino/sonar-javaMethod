    @Override
    public boolean equals(Object obj) {
      if (obj == null) { return false; }
      if (obj == this) { return true; }
      if (!(obj instanceof ArrayType)) {
           return false;
      }
      ArrayType rhs = (ArrayType) obj;
      return new EqualsBuilder()
          .append(elementType, rhs.elementType)
          .isEquals();
    }

