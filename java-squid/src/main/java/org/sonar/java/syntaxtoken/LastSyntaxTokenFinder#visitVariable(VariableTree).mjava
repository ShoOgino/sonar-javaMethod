  @Override
  public void visitVariable(VariableTree tree) {
    SyntaxToken endToken = tree.endToken();
    if(endToken == null) {
      ExpressionTree initializer = tree.initializer();
      if(initializer == null) {
        scan(tree.simpleName());
      } else {
        scan(initializer);
      }
    } else {
      lastSyntaxToken = endToken;
    }
  }

