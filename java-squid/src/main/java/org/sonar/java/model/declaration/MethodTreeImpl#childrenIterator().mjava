  @Override
  public Iterator<Tree> childrenIterator() {
    return Iterators.concat(
      Iterators.forArray(
        modifiers,
        returnType,
        simpleName
      ),
      parameters.iterator(),
      Iterators.singletonIterator(block),
      throwsClauses.iterator(),
      Iterators.singletonIterator(defaultValue)
    );
  }

