  @Nullable
  @Override
  public SyntaxToken openParenToken() {
    if (!resources.isEmpty()) {
      return InternalSyntaxToken.createLegacy(getAstNode()
        .getFirstChild(JavaGrammar.RESOURCE_SPECIFICATION)
        .getFirstChild(JavaPunctuator.LPAR));
    } else {
      return null;
    }
  }

