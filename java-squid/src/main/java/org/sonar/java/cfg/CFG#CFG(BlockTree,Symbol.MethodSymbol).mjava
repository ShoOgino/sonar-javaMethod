  private CFG(BlockTree tree, Symbol.MethodSymbol symbol) {
    methodSymbol = symbol;
    exitBlocks.add(createBlock());
    currentBlock = createBlock(exitBlock());
    for (StatementTree statementTree : Lists.reverse(tree.body())) {
      build(statementTree);
    }
    prune();
    computePredecessors(blocks);
  }

