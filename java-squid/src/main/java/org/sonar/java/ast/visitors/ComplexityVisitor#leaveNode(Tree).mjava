  @Override
  public void leaveNode(Tree tree) {
    switch (tree.kind()) {
      case CLASS:
      case ENUM:
      case ANNOTATION_TYPE:
        classTrees.pop();
        break;
      case METHOD:
      case CONSTRUCTOR:
        leaveMethod((MethodTree) tree);
        break;
      default:
        // nothing to do
    }
  }

