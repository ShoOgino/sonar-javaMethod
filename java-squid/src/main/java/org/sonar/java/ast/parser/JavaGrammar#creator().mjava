  public ExpressionTree creator() {
    return b.<ExpressionTree>nonterminal(JavaLexer.CREATOR)
      .is(
        f.completeCreator(
          b.optional(typeArguments()),
          b.firstOf(
            f.newClassCreator(qualifiedIdentifier(), classCreatorRest()),
            f.newArrayCreator(
              b.firstOf(
                qualifiedIdentifier(),
                basicType()),
              arrayCreatorRest()))));
  }

