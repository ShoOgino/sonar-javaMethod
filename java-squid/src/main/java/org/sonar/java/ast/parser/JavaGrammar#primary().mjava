  public ExpressionTree primary() {
    return b.<ExpressionTree>nonterminal(JavaLexer.PRIMARY)
      .is(
        b.firstOf(
          lambdaExpression(),
          identifierOrMethodInvocation(),
          parenthesizedExpression(),
          literal(),
          newExpression(),
          basicClassExpression(),
          voidClassExpression()));
  }

