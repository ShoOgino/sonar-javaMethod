  private static Tree createParentLink(JavaTree parent) {
    if (!parent.isLeaf()) {
      for (Iterator<Tree> iter = parent.childrenIterator(); iter.hasNext();) {
        JavaTree next = (JavaTree) iter.next();
        if (next != null) {
          next.setParent(parent);
          createParentLink(next);
        }
      }
    }
    return parent;
  }

