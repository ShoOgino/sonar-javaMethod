  @Override
  public void visitFile(AstNode astNode) {
    if (astNode == null) {
      // parse error
      semanticModel = null;
      return;
    }

    try {
      semanticModel = SemanticModel.createFor(astNode);
    } catch (Exception e) {
      LOG.error("Unable to create symbol table for " + getContext().getFile(), e);
      semanticModel = null;
      return;
    }
  }

