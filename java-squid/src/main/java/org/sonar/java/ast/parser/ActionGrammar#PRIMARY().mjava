  public ExpressionTree PRIMARY() {
    return b.<ExpressionTree>nonterminal(JavaGrammar.PRIMARY)
      .is(
        b.firstOf(
          LAMBDA_EXPRESSION(),
          MEMBER_SELECT_OR_METHOD_INVOCATION(),
          PARENTHESIZED_EXPRESSION(),
          LITERAL(),
          NEW_EXPRESSION(),
          BASIC_CLASS_EXPRESSION(),
          VOID_CLASS_EXPRESSION()));
  }

