  boolean isSubClass(JavaSymbol.TypeJavaSymbol c, JavaSymbol base) {
    if (c == null) {
      return false;
    }
    if (c == base) {
      return true;
    } else if ((base.flags() & Flags.INTERFACE) != 0) {
      return isSubClass(superclassSymbol(c), base);
    } else {
      return isSubClass(superclassSymbol(c), base);
    }
  }

