  @Test
  public void ok() {
    LexerlessGrammarBuilder b = JavaGrammar.createGrammarBuilder();

    assertThat(b, JavaGrammar.TYPE_DECLARATION)
      .matches("class Foo {}")
      .matches("enum Foo {}")
      .matches("interface Foo {}")
      .matches("@interface Foo {}")
      .matches("public class Foo {}")
      .matches("public private class Foo {}")
      .matches(";");
  }

