  @Test
  public void ok() {
    LexerlessGrammarBuilder b = JavaLexer.createGrammarBuilder();

    assertThat(b, JavaLexer.ENUM_CONSTANT)
      .matches("identifier")
      .matches("identifier,")
      .matches("@Foo identifier")
      .matches("@Foo identifier()")
      .matches("@Foo identifier {}")
      .matches("@Foo identifier() {}");
  }

