  @Test
  public void nok() {
    assertThat(g.rule(JavaTokenType.INTEGER_LITERAL))
        .notMatches("0.1")
        .notMatches("_0")
        .notMatches("_d")
        .notMatches("._list");
    assertThat(g.rule(JavaTokenType.LONG_LITERAL))
        .notMatches("0.1")
        .notMatches("_0")
        .notMatches("_d")
        .notMatches("._list");
  }

