  @Test
  public void realLife() {
    assertThat(g.rule(JavaGrammar.FOR_STATEMENT))
        .matches("for (int i = 0; i < 10; i++) {} ")
        .matches("for (; i < 10; i++) {} ")
        .matches("for (;; i++) {} ")
        .matches("for (;;) {} ")
        .matches("for (File file : files) {} ");
  }

