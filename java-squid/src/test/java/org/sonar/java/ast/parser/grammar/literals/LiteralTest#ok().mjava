  @Test
  public void ok() {
    g.trueKeyword.mock();
    g.falseKeyword.mock();
    g.nullKeyword.mock();
    g.characterLiteral.mock();
    g.stringLiteral.mock();
    g.floatLiteral.mock();
    g.longLiteral.mock();
