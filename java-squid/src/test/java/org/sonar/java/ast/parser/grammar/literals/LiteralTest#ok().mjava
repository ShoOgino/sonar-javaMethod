    assertThat(g.literal)
        .matches("trueKeyword")
        .matches("falseKeyword")
        .matches("nullKeyword")
        .matches("characterLiteral")
        .matches("stringLiteral")
        .matches("floatLiteral")
        .matches("longLiteral")
        .matches("integerLiteral");
  }

