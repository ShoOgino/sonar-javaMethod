  @Test
  public void nok() {
    assertThat(g.doubleLiteral)
        .notMatches("0xAF.f")
        .notMatches("0xAF.BCf")
        .notMatches("0xAF.")
        .notMatches("0XAF.BC")
        .notMatches("0xAF");
    assertThat(g.floatLiteral)
        .notMatches("0xAF.f")
        .notMatches("0xAF.BCf")
        .notMatches("0xAF.")
        .notMatches("0XAF.BC")
        .notMatches("0xAF");
  }

