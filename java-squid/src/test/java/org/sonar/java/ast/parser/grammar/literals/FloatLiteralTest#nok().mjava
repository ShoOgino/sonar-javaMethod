  @Test
  public void nok() {
    assertThat(g.rule(JavaTokenType.DOUBLE_LITERAL))
        .notMatches("0xAF.f")
        .notMatches("0xAF.BCf")
        .notMatches("0xAF.")
        .notMatches("0XAF.BC")
        .notMatches("0xAF");
    assertThat(g.rule(JavaTokenType.FLOAT_LITERAL))
        .notMatches("0xAF.f")
        .notMatches("0xAF.BCf")
        .notMatches("0xAF.")
        .notMatches("0XAF.BC")
        .notMatches("0xAF");
  }

