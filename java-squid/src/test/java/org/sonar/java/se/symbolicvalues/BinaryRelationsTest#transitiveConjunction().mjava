    checkConjunction(LESS_THAN_OR_EQUAL, GREATER_THAN_OR_EQUAL, EQUAL, FULFILLED);
    checkConjunction(LESS_THAN_OR_EQUAL, NOT_EQUAL, LESS_THAN, FULFILLED);
