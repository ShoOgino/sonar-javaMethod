  @Test
  public void equality_expression() {
    b.setRootRule(JavaGrammar.EQUALITY_EXPRESSION);

    // object, object = boolean
    // boolean, boolean = boolean
    // double, double = boolean
    // float, float = boolean
    // long, long = boolean
    // int, int = boolean
    assertThat(typeOf("42 == 42")).isSameAs(symbols.unknownType);
    assertThat(typeOf("42 != 42")).isSameAs(symbols.unknownType);
  }

