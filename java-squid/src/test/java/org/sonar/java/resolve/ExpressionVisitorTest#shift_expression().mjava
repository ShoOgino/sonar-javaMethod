  @Test
  public void shift_expression() {
    b.setRootRule(JavaGrammar.SHIFT_EXPRESSION);

    // long, long = long
    // int, long = int
    // long, int = long
    // int, int = int
    assertThat(typeOf("42 << 42")).isSameAs(symbols.unknownType);
    assertThat(typeOf("42 >> 42")).isSameAs(symbols.unknownType);
    assertThat(typeOf("42 >>> 42")).isSameAs(symbols.unknownType);
  }

