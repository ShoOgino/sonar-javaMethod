  @Test
  public void verify_generic_substitution() {
    MyVisitor myVisitor = new MyVisitor();
    JavaAstScanner.scanSingleFileForTests(
      new File("src/test/java/org/sonar/java/resolve/targets/bytecodeGenerics/Main.java"),
