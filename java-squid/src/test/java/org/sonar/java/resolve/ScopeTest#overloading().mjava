  @Test
  public void overloading() {
    Scope scope = new Scope(owner);

    Symbol first = new Symbol(0, 0, "overloaded", null);
    scope.enter(first);

    Symbol second = new Symbol(0, 0, "overloaded", null);
    scope.enter(second);

    assertThat(scope.lookup("overloaded")).containsOnly(first, second);
  }

