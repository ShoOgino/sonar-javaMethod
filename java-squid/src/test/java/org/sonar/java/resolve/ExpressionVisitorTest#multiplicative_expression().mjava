  @Test
  public void multiplicative_expression() {
    b.setRootRule(JavaGrammar.MULTIPLICATIVE_EXPRESSION);

    assertThat(typeOf("42 * 42")).isSameAs(symbols.unknownType);
    assertThat(typeOf("42 / 42")).isSameAs(symbols.unknownType);
    assertThat(typeOf("42 % 42")).isSameAs(symbols.unknownType);
  }

