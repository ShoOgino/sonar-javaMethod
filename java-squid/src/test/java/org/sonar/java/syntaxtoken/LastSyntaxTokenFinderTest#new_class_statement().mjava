  @Test
  public void new_class_statement() {
    String p =
      "class Foo {"
        + "  void foo(List list) {"
        + "    new Foo();"
        + "  }"
        + "}";
    assertFirstStatementlastTokenValue(p, ")");

    p =
      "class T {"
        + "  T m() {"
        + "    this.new T(true, false) {};"
        + "  }"
        + "}";
    assertFirstStatementlastTokenValue(p, "}");
  }

