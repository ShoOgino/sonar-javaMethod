  @Test
  public void static_initializer() {
    String p =
      "class Foo {"
        + "  static {"
        + "  }"
        + "}";
    ClassTree classTree = getFirstClass(getCompilationUnit(p));
    SyntaxToken lastToken = getLastSyntaxToken(classTree.members().get(0));
    assertThat(lastToken.text()).isEqualTo("}");
  }

