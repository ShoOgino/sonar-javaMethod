  @Test
  public void union_type() {
    String p =
      "class Foo {"
        + "  void foo() {"
        + "    try {"
        + "    } catch (IOException | SQLException ex) {"
        + "    }"
        + "  }"
        + "}";
    assertFirstTokenValue(((TryStatementTree) getFirstStatement(p)).catches().get(0).parameter(), "IOException");
  }

