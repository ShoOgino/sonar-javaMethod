  void nestedIFs() {
    if (choice.equals(choice))   // compliant, don't count nesting
      if ("blue".equals(choice)) { // Noncompliant
        dispenseBlue();
      } else if ("yellow".equals(choice)) {
        dispenseYellow();
      } else if ("blue".equals(choice)) {
        dispenseBlue();
      }
    else if ("blue".equals(choice)) {
      dispenseBlue();
    }
  }

