  void compliant(String str) {
    str.matches("(.)\\1");
    str.matches("(?:(.)\\1)*");
    str.matches("(.)\\1(.)\\2");
    str.matches("(?:x(.)|\\1)*");
    str.matches("(?<name>)\\k<name>");
    str.matches("(?<name>)\\1");
    // This produces an FP in IntelliJ
    str.matches("(?:\\1|x(.))*");
    str.matches("(?:\\1|x(.))+");
    str.matches("(?:\\1|x(.)){0,2}");
    str.matches("(?:\\1|x(.)){1,2}");
    str.matches("(?:\\1\\2|(x)(.))*");
    str.matches("(.)(?:\\1\\2|x(.))*");
  }

