  private String programPoint() {
    String tree = "";
    if (pp.i < pp.block.elements().size()) {
      Tree syntaxNode = ((CFG.Block) pp.block).elements().get(pp.i);
      tree = "" + syntaxNode.kind() + " L#" + syntaxNode.firstToken().line();
    }
    return programPointKey() + "  " + tree;
  }

