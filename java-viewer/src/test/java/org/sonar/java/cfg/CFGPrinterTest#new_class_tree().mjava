  @Test
  public void new_class_tree() {
    String code = "class A {"
      + "  void foo() {"
      + "    A a = new A();"
      + "  }"
      + "}";

    Viewer.Base base = new Viewer.Base(code);

    assertThat(CFGPrinter.toString(base.cfgFirstMethod)).isEqualTo(
      "Starts at B1\n"
        + "\n"
        + "B1\n"
        + "0:\tNEW_CLASS                           \tnew A ( )\n"
        + "1:\tVARIABLE                            \tA a\n"
        + "\tjumps to: B0\n"
        + "\n"
        + "B0 (Exit):\n"
        + "\n");
  }

