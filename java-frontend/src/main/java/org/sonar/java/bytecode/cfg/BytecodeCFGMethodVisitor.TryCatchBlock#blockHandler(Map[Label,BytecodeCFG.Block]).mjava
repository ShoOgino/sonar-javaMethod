    BytecodeCFG.Block blockHandler(Map<Label, BytecodeCFG.Block> blockByLabel) {
      BytecodeCFG.Block blockHandler = blockByLabel.get(handler);
      String exType = type;
      if(exType == null) {
        exType = "!UncaughtException!";
      }
      blockHandler.exceptionType = exType;
      return blockHandler;
    }

