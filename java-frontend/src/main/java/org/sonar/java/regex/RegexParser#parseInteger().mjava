  @CheckForNull
  private RegexToken parseInteger() {
    int startIndex = index;
    if (!isAsciiDigit(currentChar())) {
      return null;
    }
    while(isAsciiDigit(currentChar())) {
      index++;
    }
    IndexRange range = new IndexRange(startIndex, index);
    return new RegexToken(source, range);
  }

