  @Override
  public boolean equals(Object obj) {
    if (this == obj) {
      return true;
    }
    if (obj instanceof SinglyLinkedList) {
      final SinglyLinkedList other = (SinglyLinkedList) obj;
      return this.element.equals(other.element)
        && Objects.equals(this.next, other.next);
    }
    return false;
  }

