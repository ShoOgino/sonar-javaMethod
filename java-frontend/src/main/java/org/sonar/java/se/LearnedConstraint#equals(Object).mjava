  @Override
  public boolean equals(Object o) {
    if (this == o) {
      return true;
    }
    if (o == null || getClass() != o.getClass()) {
      return false;
    }
    LearnedConstraint that = (LearnedConstraint) o;
    return Objects.equals(sv, that.sv) &&
      Objects.equals(constraint, that.constraint);
  }

