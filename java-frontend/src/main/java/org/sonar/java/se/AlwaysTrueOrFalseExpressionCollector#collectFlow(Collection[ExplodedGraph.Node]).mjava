  private static Set<List<JavaFileScannerContext.Location>> collectFlow(Collection<ExplodedGraph.Node> nodes) {
    return nodes.stream()
      .map(AlwaysTrueOrFalseExpressionCollector::flowFromNode)
      .flatMap(Set::stream)
      .filter(f -> !f.isEmpty())
      .collect(Collectors.toSet());
  }

