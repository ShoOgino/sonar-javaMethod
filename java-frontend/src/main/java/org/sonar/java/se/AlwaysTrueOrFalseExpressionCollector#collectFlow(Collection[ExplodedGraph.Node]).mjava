  private static Set<Flow> collectFlow(Collection<ExplodedGraph.Node> nodes) {
    return nodes.stream()
      .map(AlwaysTrueOrFalseExpressionCollector::flowFromNode)
      .flatMap(Set::stream)
      .limit(FlowComputation.MAX_RETURNED_FLOWS)
      .filter(f -> !f.isEmpty())
      .collect(Collectors.toSet());
  }

