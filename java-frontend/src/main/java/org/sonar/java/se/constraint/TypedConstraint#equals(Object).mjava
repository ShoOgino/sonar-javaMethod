  @Override
  public boolean equals(Object o) {
    if (this == o) {
      return true;
    }
    if (o == null || getClass() != o.getClass()) {
      return false;
    }
    if (type.isUnknown()) {
      return false;
    }
    TypedConstraint that = (TypedConstraint) o;
    return type.equals(that.type);
  }

