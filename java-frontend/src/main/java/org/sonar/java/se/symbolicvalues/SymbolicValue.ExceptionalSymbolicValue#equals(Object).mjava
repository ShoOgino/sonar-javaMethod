    @Override
    public boolean equals(Object o) {
      if (this == o) {
        return true;
      }
      if (o == null || getClass() != o.getClass()) {
        return false;
      }
      ExceptionalSymbolicValue that = (ExceptionalSymbolicValue) o;
      return id() == that.id() && Objects.equals(exceptionType, that.exceptionType);
    }

