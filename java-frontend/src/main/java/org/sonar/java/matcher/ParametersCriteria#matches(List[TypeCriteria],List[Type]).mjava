  static boolean matches(List<TypeCriteria> expectedTypes, List<Type> actualTypes) {
    if (actualTypes.size() != expectedTypes.size()) {
      return false;
    }
    for (int i = 0; i < actualTypes.size(); i++) {
      if (!expectedTypes.get(i).test(actualTypes.get(i))) {
        return false;
      }
    }
    return true;
  }

