  public Tree RESOURCE() {
    return b.<Tree>nonterminal(JavaLexer.RESOURCE)
      .is(b.firstOf(
        f.newResource(
          MODIFIERS(),
          b.firstOf(
            VAR_TYPE(),
            TYPE_QUALIFIED_IDENTIFIER()),
          VARIABLE_DECLARATOR_ID(),
          b.token(JavaPunctuator.EQU),
          EXPRESSION()),
        PRIMARY_WITH_SELECTOR()));
  }

