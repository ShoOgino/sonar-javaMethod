  public VariableTreeImpl newVariableDeclaratorId(InternalSyntaxToken identifierToken,
    Optional<List<Tuple<Optional<List<AnnotationTreeImpl>>, Tuple<InternalSyntaxToken, InternalSyntaxToken>>>> dims) {
