  public ExpressionTree CREATOR() {
    return b.<ExpressionTree>nonterminal(JavaLexer.CREATOR)
      .is(
        b.firstOf(
          f.newClassCreator(b.optional(TYPE_ARGUMENTS()), TYPE_QUALIFIED_IDENTIFIER(), CLASS_CREATOR_REST()),
          f.newArrayCreator(
            b.firstOf(
              TYPE_QUALIFIED_IDENTIFIER(),
              BASIC_TYPE()),
            ARRAY_CREATOR_REST())));
  }

