  @Override
  @Nullable
  public Boolean isOverriding() {
    if (isStatic() || isPrivate()) {
      return false;
    }
    if (isAnnotatedOverride()) {
      return true;
    }
    if (symbol() == null) {
      return null;
    }
    Symbol.MethodSymbol methodSymbol = symbol().overriddenSymbol();
    if (methodSymbol != null) {
      return methodSymbol.isUnknown() ? null : true;
    }
    return false;
  }

