  public boolean isSubtypeOfBound(JavaType type) {
    switch (boundType) {
      case SUPER:
        return !boundIsTypeVarAndNotType(type) && bound.isSubtypeOf(type);
      case EXTENDS:
        return !boundIsTypeVarAndNotType(type) && type.isSubtypeOf(bound);
      case UNBOUNDED:
      default:
        return true;
    }
  }

