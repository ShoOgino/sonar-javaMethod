  @Test
  void escapedMetaCharacters() {
    RegexParserTestUtils.assertCharacter('\\', "\\\\\\\\");
    RegexParserTestUtils.assertCharacter('.', "\\\\.");
    RegexParserTestUtils.assertCharacter('(', "\\\\(");
    RegexParserTestUtils.assertCharacter(')', "\\\\)");
    RegexParserTestUtils.assertCharacter('[', "\\\\[");
    RegexParserTestUtils.assertCharacter(']', "\\\\]");
    RegexParserTestUtils.assertCharacter('{', "\\\\{");
    RegexParserTestUtils.assertCharacter('}', "\\\\}");
  }

