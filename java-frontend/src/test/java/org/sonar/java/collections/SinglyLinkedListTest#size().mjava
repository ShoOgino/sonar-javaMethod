  @Test
  public void size() {
    PStack<Object> s = PCollections.emptyStack();
    assertThat(s.size()).isEqualTo(0);
    s = s.push(new Object());
    assertThat(s.size()).isEqualTo(1);
    s = s.push(new Object());
    assertThat(s.size()).isEqualTo(2);
    s = s.pop().pop();
    assertThat(s.size()).isEqualTo(0);
  }

