    @Override
    public void visitNode(Tree tree) {
      int line = ((JavaTree) tree).getLine();
      Symbol.MethodSymbol symbol;
      if (tree.is(Tree.Kind.METHOD)) {
        symbol = ((MethodTree) tree).symbol();
      } else {
        symbol = (Symbol.MethodSymbol) ((MethodInvocationTree) tree).symbol();
      }

      AbstractObjectAssert<?, Symbol.MethodSymbol> assertion = assertThat(symbol.overriddenSymbol()).as("Method at line " + line);
      if (overrides.contains(line)) {
        assertion.isNotNull();
      } else if (unknowns.contains(line)) {
        assertion.isNull();
        assertThat(symbol.type()).isEqualTo(Symbols.unknownType);
      } else {
        assertion.isNull();
      }
    }

