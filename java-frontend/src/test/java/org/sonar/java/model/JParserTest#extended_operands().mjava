  /**
   * @see org.eclipse.jdt.core.dom.InfixExpression#extendedOperands()
   */
  @Test
  public void extended_operands() {
    test("class C { void m() { m( 1 - 2 - 3 ); } }");

    // no extendedOperands in case of parentheses:
    test("class C { void m() { m( (1 - 2) - 3 ); } }");
    test("class C { void m() { m( 1 - (2 - 3) ); } }");
  }

