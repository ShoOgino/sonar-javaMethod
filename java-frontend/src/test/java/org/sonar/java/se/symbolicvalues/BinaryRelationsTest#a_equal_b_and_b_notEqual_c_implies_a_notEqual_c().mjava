  @Test
  public void a_equal_b_and_b_notEqual_c_implies_a_notEqual_c() {
    BinaryRelation r1 = relation(EQUAL, SVa, SVb);
    BinaryRelation r2 = relation(NOT_EQUAL, SVb, SVc);
