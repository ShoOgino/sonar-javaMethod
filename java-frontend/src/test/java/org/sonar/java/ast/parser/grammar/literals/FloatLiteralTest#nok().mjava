  @Test
  public void nok() {
    assertThat(JavaTokenType.DOUBLE_LITERAL)
      .notMatches("0xAF.f")
      .notMatches("0xAF.BCf")
      .notMatches("0xAF.")
      .notMatches("0XAF.BC")
      .notMatches("0xAF");
    assertThat(JavaTokenType.FLOAT_LITERAL)
      .notMatches("0xAF.f")
      .notMatches("0xAF.BCf")
      .notMatches("0xAF.")
      .notMatches("0XAF.BC")
      .notMatches("0xAF");
  }

