  @Test
  public void ok() {
    LexerlessGrammarBuilder b = JavaLexer.createGrammarBuilder();

    assertThat(b, JavaLexer.FORMAL_PARAMETERS)
      .matches("( )")
      .matches("( int foo, int... bar )")
        // Java 4 syntax : enum is not a keyword
      .matches("( int enum, int... bar )");
  }

