  @Test
  public void lambda() {
    String p =
      "class Foo {"
        + "  void foo(Person p) {"
        + "    p -> p.printPerson();"
        + "  }"
        + "}";
    assertFirstStatementFirstTokenValue(p, "p");

    p =
      "class Foo {"
        + "  void foo(Person p1, Person p2) {"
        + "    (p1, p2) -> p1.isTallerThan(p2);"
        + "  }"
        + "}";
    assertFirstStatementFirstTokenValue(p, "(");
  }

