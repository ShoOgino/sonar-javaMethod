  public void qix(java.util.List<String> list) {
    list.stream()
      .filter(s -> s.length() > 42)
      .map(s -> stringToBoolean(s));

    list.stream()
      .filter(s -> s.length() > 42)
      .map(s -> stringToBoolean(s))
      .map(b -> booleanToInt(b));

    list.stream()
      .map(String::length)
      .filter(x -> intToInt(x) > 0);
  }

