  void qix(Set<String> set) {
    set.stream()
      .map(key -> key.startsWith("hello") ? key.length() : key)
      .collect(Collectors.toList())
      .forEach(A::foo);

    set.stream()
      .flatMap(key -> key.startsWith("hello") ? Stream.empty() : Stream.of(Integer.parseInt(key)))
      .collect(Collectors.toList())
      .forEach(A::bar);
  }

