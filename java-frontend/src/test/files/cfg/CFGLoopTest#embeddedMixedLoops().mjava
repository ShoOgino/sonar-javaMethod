  void embeddedMixedLoops() {
    for (int i = 0; i < 10; i++) {
      int n = 0;
      while (n++ < 10) {
        if (n < 5) {
          doSomething();
        } else {
          doSomeOtherThing();
        }
        n += 1;
      }
      if (i < 5) {
        doSomething();
      } else {
        doSomeOtherThing();
      }
    }
  }

