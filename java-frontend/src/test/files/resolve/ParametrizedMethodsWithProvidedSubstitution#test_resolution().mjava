  void test_resolution() {
    this.<String>f1("String");
    this.<B>f1(new B());
    this.<A>f1(new A());

    this.<Integer>f2(new B<Integer>());
    this.<String>f2(new D());
    this.<A>f2(new B());
    this.<A>f2(new A());
    
    this.<Integer>f3(0, new B<Integer>());

    this.<String>f4(new String[0]);
