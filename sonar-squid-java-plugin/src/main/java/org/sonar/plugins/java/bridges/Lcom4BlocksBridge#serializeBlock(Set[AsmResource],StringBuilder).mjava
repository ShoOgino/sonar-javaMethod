  private void serializeBlock(Set<AsmResource> block, StringBuilder sb) {
    List<AsmResource> sortedResources = sortResourcesInBlock(block);
    int indexResource = 0;
    for (AsmResource resource : sortedResources) {
      if (indexResource++ > 0) {
        sb.append(',');
      }
      serializeResource(resource, sb);
    }
  }

