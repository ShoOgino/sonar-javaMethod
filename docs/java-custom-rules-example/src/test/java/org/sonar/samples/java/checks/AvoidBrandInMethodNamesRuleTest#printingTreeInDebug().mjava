  @Test
  public void printingTreeInDebug() {
    // re-execute the test
    JavaCheckVerifier.newVerifier()
    .onFile("src/test/files/AvoidBrandInMethodNamesRule.java")
    .withCheck(new AvoidBrandInMethodNamesRule())
    .verifyIssues();

    // verify that the tree has been correctly displayed in DEBUG
    assertThat(logTester.logs(LoggerLevel.DEBUG))
      .hasSize(1)
      .containsOnly("CompilationUnitTree : [\n"
        + "  ClassTree\n"
        + "    ModifiersTree\n"
        + "    IdentifierTree\n"
        + "    TypeParameters : [\n"
        + "    VariableTree\n"
        + "      ModifiersTree\n"
        + "      PrimitiveTypeTree\n"
        + "      IdentifierTree\n"
        + "    MethodTree\n"
        + "      ModifiersTree\n"
        + "      TypeParameters\n"
        + "      PrimitiveTypeTree\n"
        + "      IdentifierTree\n"
        + "      BlockTree\n"
        + "    MethodTree\n"
        + "      ModifiersTree\n"
        + "      TypeParameters\n"
        + "      PrimitiveTypeTree\n"
        + "      IdentifierTree\n"
        + "      BlockTree\n"
        + "    MethodTree\n"
        + "      ModifiersTree\n"
        + "      TypeParameters\n"
        + "      PrimitiveTypeTree\n"
        + "      IdentifierTree\n"
        + "      BlockTree\n"
        + "    ]\n"
        + "  ]\n");
  }

