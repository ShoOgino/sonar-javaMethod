  @Test
  public void new_array() {
    String p =
      "class Foo {"
        + "  void foo() {"
        + "    new int[3];"
        + "  }"
        + "}";
    assertFirstStatementlastTokenValue(p, "3");

    p =
      "class Foo {"
        + "  void foo() {"
        + "    new int[] {1, 2, 3};"
        + "  }"
        + "}";
    assertFirstStatementlastTokenValue(p, "3");
  }

