  @Test
  public void parameterized_type() {
    String p = "class Foo {"
      + "  Collection<T> foo(T t) {"
      + "  }"
      + "}";
    MethodTree m = getFirstMethod(getCompilationUnit(p));
    SyntaxToken lastToken = getLastSyntaxToken(m.returnType());
    assertThat(lastToken.text()).isEqualTo(">");
  }

