  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/SpecializedFunctionalInterfacesCheck.java"))
      .withCheck(new SpecializedFunctionalInterfacesCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/SpecializedFunctionalInterfacesCheck.java"))
      .withCheck(new SpecializedFunctionalInterfacesCheck())
      .withoutSemantic()
      .verifyNoIssues();
