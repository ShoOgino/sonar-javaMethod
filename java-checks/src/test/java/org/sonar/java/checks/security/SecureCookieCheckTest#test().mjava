  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/security/SecureCookieCheck.java")
      .withCheck(new SecureCookieCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/security/SecureCookieCheck.java")
      .withCheck(new SecureCookieCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

