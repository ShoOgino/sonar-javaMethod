  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/AtLeastOneConstructorCheck.java")
      .withCheck(new AtLeastOneConstructorCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/AtLeastOneConstructorCheck.java")
      .withCheck(new AtLeastOneConstructorCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

