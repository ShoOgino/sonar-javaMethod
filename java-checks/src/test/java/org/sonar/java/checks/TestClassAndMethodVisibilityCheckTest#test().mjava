  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcePath)
      .withCheck(new TestClassAndMethodVisibilityCheck())
      .verifyIssues();

    JavaCheckVerifier.newVerifier()
      .onFile(testSourcePath)
      .withCheck(new TestClassAndMethodVisibilityCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

