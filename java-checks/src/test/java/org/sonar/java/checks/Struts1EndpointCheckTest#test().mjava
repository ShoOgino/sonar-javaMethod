  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/Struts1EndpointCheck.java")
      .withCheck(new Struts1EndpointCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/Struts1EndpointCheck.java")
      .withCheck(new Struts1EndpointCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

