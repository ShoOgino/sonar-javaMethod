  @Test
  void intersects_not_supported() {
    assertIntersects(".", "a", false).isFalse();
    assertIntersects(".", "a", true).isTrue();

    assertIntersects("a", ".", false).isFalse();
    assertIntersects("a", ".", true).isTrue();

    assertIntersects("a(?<=a)", "a", true).isTrue();
    assertIntersects("a(?<=a)", "a", false).isFalse();
  }

