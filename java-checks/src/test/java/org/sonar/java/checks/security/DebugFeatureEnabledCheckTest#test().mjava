  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/security/DebugFeatureEnabledCheck.java")
      .withCheck(new DebugFeatureEnabledCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/security/DebugFeatureEnabledCheck.java")
      .withCheck(new DebugFeatureEnabledCheck())
      .withoutSemantic()
      .verifyNoIssues();
