  @Override
  protected void scan(@Nullable Tree tree) {
    if (tree != null) {
      indent().append(tree.getClass().getSimpleName());
      AstNode node = ((JavaTree) tree).getAstNode();
      if(node!=null){
        sb.append(" ").append(node.getTokenLine());
      }
      sb.append("\n");
    }
    indentLevel++;
    super.scan(tree);
    indentLevel--;
  }

