  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/AssertsOnParametersOfPublicMethodCheck.java")
      .withCheck(new AssertsOnParametersOfPublicMethodCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/AssertsOnParametersOfPublicMethodCheck.java")
      .withCheck(new AssertsOnParametersOfPublicMethodCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

