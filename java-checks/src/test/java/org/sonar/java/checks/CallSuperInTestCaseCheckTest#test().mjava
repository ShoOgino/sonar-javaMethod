  @Test
  public void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/CallSuperInTestCaseCheck.java"))
      .withCheck(new CallSuperInTestCaseCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/CallSuperInTestCaseCheck.java"))
      .withCheck(new CallSuperInTestCaseCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

