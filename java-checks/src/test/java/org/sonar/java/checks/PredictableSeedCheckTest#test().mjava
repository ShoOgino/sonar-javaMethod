  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/PredictableSeedCheck.java")
      .withCheck(new PredictableSeedCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/PredictableSeedCheck.java")
      .withCheck(new PredictableSeedCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

