  @Test
  public void max_level_is_2() {
    DITCheck check = new DITCheck();
    check.setMax(2);

    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/Dit.java")
      .withCheck(check)
      .verifyIssues();

    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/Dit.java")
      .withCheck(check)
      .withoutSemantic()
      .verifyNoIssues();
  }

