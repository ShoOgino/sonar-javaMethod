  @Test
  public void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/security/PasswordEncoder.java"))
      .withCheck(new PasswordEncoderCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/security/PasswordEncoder.java"))
      .withCheck(new PasswordEncoderCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

