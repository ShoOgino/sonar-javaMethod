  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/ImmediateReverseBoxingCheck.java")
      .withCheck(new ImmediateReverseBoxingCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/ImmediateReverseBoxingCheck.java")
      .withCheck(new ImmediateReverseBoxingCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

