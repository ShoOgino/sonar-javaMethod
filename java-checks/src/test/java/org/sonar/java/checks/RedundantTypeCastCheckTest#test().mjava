  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/RedundantTypeCastCheck.java")
      .withCheck(new RedundantTypeCastCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/RedundantTypeCastCheck.java")
      .withCheck(new RedundantTypeCastCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

