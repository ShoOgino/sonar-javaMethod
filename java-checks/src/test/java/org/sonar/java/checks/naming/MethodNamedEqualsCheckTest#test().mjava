  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/naming/MethodNamedEqualsCheck.java")
      .withCheck(new MethodNamedEqualsCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/naming/MethodNamedEqualsCheck.java")
      .withCheck(new MethodNamedEqualsCheck())
      .withoutSemantic()
      .verifyNoIssues();
