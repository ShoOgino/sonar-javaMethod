  @Test
  public void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/ThreadAsRunnableArgumentCheck.java")
      .withCheck(new ThreadAsRunnableArgumentCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/ThreadAsRunnableArgumentCheck.java")
      .withCheck(new ThreadAsRunnableArgumentCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

