  @Test
  void test() {
    TooManyMethodsCheck check = new TooManyMethodsCheck();
    check.maximumMethodThreshold = 4;
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/TooManyMethodsCheck.java")
      .withCheck(check)
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/TooManyMethodsCheck.java")
      .withCheck(check)
      .withoutSemantic()
      .verifyNoIssues();
  }

