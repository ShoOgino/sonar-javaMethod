  @Test
  public void java6() {
    JavaCheckVerifier.newVerifier()
      .onFile(FILENAME)
      .withCheck(new JdbcDriverExplicitLoadingCheck())
      .withJavaVersion(6)
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(FILENAME)
      .withCheck(new JdbcDriverExplicitLoadingCheck())
      .withJavaVersion(6)
      .withoutSemantic()
      .verifyNoIssues();
  }

