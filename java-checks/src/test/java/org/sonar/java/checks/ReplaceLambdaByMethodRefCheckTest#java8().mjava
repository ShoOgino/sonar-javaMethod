  @Test
  public void java8() {
    JavaCheckVerifier.newVerifier()
      .onFile(FILENAME)
      .withCheck(new ReplaceLambdaByMethodRefCheck())
      .withJavaVersion(8)
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(FILENAME)
      .withCheck(new ReplaceLambdaByMethodRefCheck())
      .withJavaVersion(8)
      .withoutSemantic()
      .verifyNoIssues();
  }

