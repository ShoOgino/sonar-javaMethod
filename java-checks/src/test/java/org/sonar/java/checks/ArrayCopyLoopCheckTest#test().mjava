  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/ArrayCopyLoopCheck.java"))
      .withCheck(new ArrayCopyLoopCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/ArrayCopyLoopCheck.java"))
      .withCheck(new ArrayCopyLoopCheck())
      .withoutSemantic()
      .verifyNoIssues();
