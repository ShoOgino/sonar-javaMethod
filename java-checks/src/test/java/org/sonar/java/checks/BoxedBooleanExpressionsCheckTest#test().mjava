  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/BoxedBooleanExpressionsCheck.java")
      .withCheck(new BoxedBooleanExpressionsCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/BoxedBooleanExpressionsCheck.java")
      .withCheck(new BoxedBooleanExpressionsCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

