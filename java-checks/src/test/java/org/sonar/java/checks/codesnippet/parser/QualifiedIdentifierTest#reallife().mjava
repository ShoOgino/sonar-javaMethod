  @Test
  public void reallife() {
    assertThat(p, parse("foo"));
    assertThat(p, parse("foo.bar"));
    assertThat(p, parse("foo.bar().baz"));
    assertThat(p, parse("foo.bar().baz.qux"));
    assertThat(p, parse("foo.bar().baz().qux"));
    assertThat(p, parse("this"));
    assertThat(p, parse("this.foo"));
    assertThat(p, parse("super"));
    assertThat(p, parse("super.foo"));
    assertThat(p, parse("super.this"));
    assertThat(p, parse("super.this.bar().this"));

    assertThat(p, notParse("foo()"));
  }

