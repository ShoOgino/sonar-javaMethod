  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/InterruptedExceptionCheck.java")
      .withCheck(new InterruptedExceptionCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/InterruptedExceptionCheck.java")
      .withCheck(new InterruptedExceptionCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

