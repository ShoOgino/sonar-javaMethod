  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/CloneOverrideCheck.java")
      .withCheck(new CloneOverrideCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/CloneOverrideCheck.java")
      .withCheck(new CloneOverrideCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

