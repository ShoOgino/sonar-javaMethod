  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/RedundantThrowsDeclaration.java")
      .withCheck(new RedundantThrowsDeclarationCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/RedundantThrowsDeclaration.java")
      .withCheck(new RedundantThrowsDeclarationCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

