  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/StaticMemberAccess.java")
      .withCheck(new StaticMemberAccessCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/StaticMemberAccess.java")
      .withCheck(new StaticMemberAccessCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

