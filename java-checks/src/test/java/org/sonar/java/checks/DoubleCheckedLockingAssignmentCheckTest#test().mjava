  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/DoubleCheckedLockingAssignment.java")
      .withCheck(new DoubleCheckedLockingAssignmentCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/DoubleCheckedLockingAssignment.java")
      .withCheck(new DoubleCheckedLockingAssignmentCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

