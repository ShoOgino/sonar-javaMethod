  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/HiddenFieldCheck.java"))
      .withCheck(new HiddenFieldCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/HiddenFieldCheck.java"))
      .withCheck(new HiddenFieldCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

