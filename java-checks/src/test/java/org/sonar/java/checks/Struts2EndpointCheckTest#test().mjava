  @Test
  public void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/Struts2EndpointCheck.java")
      .withCheck(new Struts2EndpointCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/Struts2EndpointCheck.java")
      .withCheck(new Struts2EndpointCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

