  @Test
  public void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/spring/SpringComponentScanCheck.java")
      .withCheck(new SpringComponentScanCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/spring/SpringComponentScanCheck.java")
      .withCheck(new SpringComponentScanCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

