  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/ToArrayCheck.java"))
      .withCheck(new ToArrayCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/ToArrayCheck.java"))
      .withCheck(new ToArrayCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

