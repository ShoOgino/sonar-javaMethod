  @Test
  void max_level_is_2_and_filtered() {
    DITCheck check = new DITCheck();
    check.setMax(2);
    check.setFilteredClasses("java.lang.Object");

    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/DitFiltered.java")
      .withCheck(check)
      .verifyIssues();
  }

