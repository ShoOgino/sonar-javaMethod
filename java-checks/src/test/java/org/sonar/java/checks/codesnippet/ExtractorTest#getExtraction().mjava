  @Test
  public void getExtraction() {
    assertThat(newExtractor("foo").getExtraction(Collections.EMPTY_LIST)).containsExactly();
    assertThat(newExtractor("foo").getExtraction(Lists.newArrayList(0))).containsExactly('f');
    assertThat(newExtractor("foo").getExtraction(Lists.newArrayList(0, 2))).containsExactly('f', 'o');
    assertThat(newExtractor("foo").getExtraction(Lists.newArrayList(2, 0))).containsExactly('o', 'f');
  }

