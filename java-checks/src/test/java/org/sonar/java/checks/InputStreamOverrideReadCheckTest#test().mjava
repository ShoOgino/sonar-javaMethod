  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/InputStreamOverrideReadCheck.java")
      .withCheck(new InputStreamOverrideReadCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/InputStreamOverrideReadCheck.java")
      .withCheck(new InputStreamOverrideReadCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

