  @Test
  public void version_in_range_always_match() {
    matcher = new RangedVersionMatcher("1.2", "1.5.6");
    assertMatches("1.2.1.1");
    assertMatches("1.2.1-SNAPSHOT");
    assertMatches("1.3.4");
    assertMatches("1.3");

    // bounds are included
    assertMatches("1.2");
    assertMatches("1.5.6");
  }

