  @Test
  public void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/SwitchLastCaseIsDefaultCheck.java"))
      .withCheck(new SwitchLastCaseIsDefaultCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/SwitchLastCaseIsDefaultCheck.java"))
      .withCheck(new SwitchLastCaseIsDefaultCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

