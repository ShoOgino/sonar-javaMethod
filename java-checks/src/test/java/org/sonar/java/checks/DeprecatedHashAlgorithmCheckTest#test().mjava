  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/DeprecatedHashAlgorithmCheck.java"))
      .withCheck(new DeprecatedHashAlgorithmCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/DeprecatedHashAlgorithmCheck.java"))
      .withCheck(new DeprecatedHashAlgorithmCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

