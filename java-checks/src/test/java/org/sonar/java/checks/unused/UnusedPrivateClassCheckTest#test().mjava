  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/UnusedPrivateClass.java"))
      .withCheck(new UnusedPrivateClassCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/UnusedPrivateClass.java"))
      .withCheck(new UnusedPrivateClassCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

