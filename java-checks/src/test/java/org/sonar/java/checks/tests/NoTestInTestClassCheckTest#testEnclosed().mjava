  @Test
  void testEnclosed() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/NoTestInTestClassCheckEnclosed.java")
      .withCheck(new NoTestInTestClassCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/NoTestInTestClassCheckEnclosed.java")
      .withCheck(new NoTestInTestClassCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

