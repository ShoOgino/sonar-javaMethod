  @Test
  public void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/unused/UnusedMethodParameterCheck.java")
      .withCheck(new UnusedMethodParameterCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/unused/UnusedMethodParameterCheck.java")
      .withCheck(new UnusedMethodParameterCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

