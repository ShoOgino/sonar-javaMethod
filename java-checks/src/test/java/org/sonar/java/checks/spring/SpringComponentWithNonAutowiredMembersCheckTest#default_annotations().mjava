  @Test
  public void default_annotations() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath(basePath + "DefaultAnnotations.java"))
      .withCheck(check)
      .verifyIssues();

    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath(basePath + "DefaultAnnotations.java"))
      .withCheck(check)
      .withoutSemantic()
      .verifyNoIssues();
  }

