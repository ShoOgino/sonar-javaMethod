  @Test
  public void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/security/AndroidSSLConnection.java")
      .withCheck(new AndroidSSLConnectionCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/security/AndroidSSLConnection.java")
      .withCheck(new AndroidSSLConnectionCheck())
