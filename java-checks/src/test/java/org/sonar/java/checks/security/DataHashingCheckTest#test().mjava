  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/security/DataHashingCheck.java")
      .withCheck(new DataHashingCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/security/DataHashingCheck.java")
      .withCheck(new DataHashingCheck())
      .withoutSemantic()
      .verifyNoIssues();
