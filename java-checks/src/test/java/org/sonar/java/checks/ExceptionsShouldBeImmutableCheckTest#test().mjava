  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/ExceptionsShouldBeImmutableCheck.java")
      .withCheck(new ExceptionsShouldBeImmutableCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/ExceptionsShouldBeImmutableCheck.java")
      .withCheck(new ExceptionsShouldBeImmutableCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

