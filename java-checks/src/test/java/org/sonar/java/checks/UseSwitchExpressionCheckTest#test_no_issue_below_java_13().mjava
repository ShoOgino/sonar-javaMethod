  @Test
  public void test_no_issue_below_java_13() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/UseSwitchExpressionCheck_java11.java"))
      .withCheck(new UseSwitchExpressionCheck())
      .withJavaVersion(11)
      .verifyNoIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/UseSwitchExpressionCheck_java11.java"))
      .withCheck(new UseSwitchExpressionCheck())
      .withJavaVersion(12)
      .verifyNoIssues();
  }

