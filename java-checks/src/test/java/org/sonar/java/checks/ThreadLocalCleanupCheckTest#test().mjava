  @Test
  void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/ThreadLocalCleanup.java"))
      .withCheck(new ThreadLocalCleanupCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/ThreadLocalCleanup.java"))
      .withCheck(new ThreadLocalCleanupCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

