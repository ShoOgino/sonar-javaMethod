  @Test
  public void test() {
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/OptionalAsParameterCheck.java"))
      .withCheck(new OptionalAsParameterCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile(testSourcesPath("checks/OptionalAsParameterCheck.java"))
      .withCheck(new OptionalAsParameterCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

