  @Test
  public void test() {
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/unused/UnusedLocalVariableCheck.java")
      .withCheck(new UnusedLocalVariableCheck())
      .verifyIssues();
    JavaCheckVerifier.newVerifier()
      .onFile("src/test/files/checks/unused/UnusedLocalVariableCheck.java")
      .withCheck(new UnusedLocalVariableCheck())
      .withoutSemantic()
      .verifyNoIssues();
  }

