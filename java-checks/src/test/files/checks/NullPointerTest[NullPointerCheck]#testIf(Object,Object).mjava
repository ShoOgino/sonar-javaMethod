  public void testIf(Object argument1, Object argument2) {
    argument1.hashCode(); // Compliant
    if (argument1 == null) {
      argument1.hashCode(); // Noncompliant
      argument1 = argument2;
      argument1.hashCode(); // Compliant
    } else {
      argument1.hashCode(); // Compliant
      argument1 = null;
      argument1.hashCode(); // Noncompliant
    }
    if (null != argument1) {
      argument1.hashCode(); // Compliant
      argument1 = null;
      argument1.hashCode(); // Noncompliant
    } else {
      argument1.hashCode(); // Noncompliant
      argument1 = argument2;
      argument1.hashCode(); // Compliant
    }
    argument1.hashCode(); // Compliant
  }

