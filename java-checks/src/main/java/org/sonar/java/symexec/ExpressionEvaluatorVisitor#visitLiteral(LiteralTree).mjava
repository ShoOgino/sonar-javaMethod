  @Override
  public void visitLiteral(LiteralTree tree) {
    if ("false".equals(tree.value())) {
      falseStates.add(currentState);
    } else if ("true".equals(tree.value())) {
      trueStates.add(currentState);
    } else {
      falseStates.add(currentState);
      trueStates.add(currentState);
    }
  }

