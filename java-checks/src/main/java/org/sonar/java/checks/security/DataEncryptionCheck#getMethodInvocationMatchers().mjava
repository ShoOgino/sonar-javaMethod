  @Override
  protected MethodMatchers getMethodInvocationMatchers() {
    return MethodMatchers.or(
      MethodMatchers.create()
        .ofTypes("javax.crypto.Cipher")
        .names("getInstance")
        .withAnyParameters()
        .build(),
      MethodMatchers.create()
        .ofTypes("org.apache.commons.crypto.utils.Utils")
        .names("getCipherInstance")
        .withAnyParameters()
        .build());
  }

