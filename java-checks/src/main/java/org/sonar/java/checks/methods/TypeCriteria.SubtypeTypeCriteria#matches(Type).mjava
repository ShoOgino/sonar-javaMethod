    @Override
    public boolean matches(Type type) {
      if (type.is(superTypeName)) {
        return true;
      }
      if (type.isTagged(Type.CLASS)) {
        for (Type.ClassType classType : type.getSymbol().superTypes()) {
          if (classType.is(superTypeName)) {
            return true;
          }
        }
      }
      return false;
    }

