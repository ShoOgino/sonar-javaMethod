    @Override
    public void visitMethodInvocation(MethodInvocationTree tree) {
      if (EQUALS_MATCHER.matches(tree)) {
        if (isInvocationOnArgument(tree)) {
          typeChecked = true;
        } else if (isArgument(tree.arguments().get(0), parameterSymbol)) {
          typeChecked = true;
        }
      }
    }

