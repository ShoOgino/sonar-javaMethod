  private static boolean isNumerical(Type argType) {
    return argType.isNumerical()
      || isOneOf(argType,
        "java.math.BigInteger",
        "java.math.BigDecimal",
        "java.lang.Byte",
        "java.lang.Short",
        "java.lang.Integer",
        "java.lang.Long",
        "java.lang.Float",
        "java.lang.Double");
  }

