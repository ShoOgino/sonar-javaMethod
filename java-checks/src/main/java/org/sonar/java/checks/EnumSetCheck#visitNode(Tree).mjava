  @Override
  public void visitNode(Tree tree) {
    if(!hasSemantic()) {
      return;
    }
    VariableTree variableTree = (VariableTree) tree;
    ExpressionTree initializer = variableTree.initializer();
    if (initializer != null) {
      checkIssue(initializer.symbolType(), initializer, variableTree.type());
    }
  }

