  @Override
  public boolean equals(Object o) {
    if (this == o) {
      return true;
    } else if (o == null || getClass() != o.getClass()) {
      return false;
    } else {
      OrderedStatePair that = (OrderedStatePair) o;
      return state1 == that.state1 && state2 == that.state2;
    }
  }

