  private boolean isComparable(ClassTree tree) {
    Symbol.TypeSymbol typeSymbol = ((ClassTreeImpl) tree).getSymbol();
    if (typeSymbol == null) {
      return false;
    }
    for (Type type : typeSymbol.getInterfaces()) {
      if ("Comparable".equals(type.getSymbol().getName())) {
        return true;
      }
    }
    return false;
  }

