  public void visitMethod(MethodTree tree) {
    for (VariableTree var: tree.parameters()) {
      checkMember(var);
    }

    if (tree.block() != null && tree.block().body() != null && !tree.block().body().isEmpty()) {
      for (StatementTree stmt : tree.block().body()) {
        checkMember(stmt);
      }
    }
    
    super.visitMethod(tree);
  }

