  private static boolean isMemberAutowired(Tree member) {
    Symbol s = null;
    if (member.is(Kind.VARIABLE)) {
      s = ((VariableTree) member).symbol();
    } else if (member.is(Kind.METHOD)) {
      s = ((MethodTree) member).symbol();
    }
    return s != null && !s.isStatic() && isAutowired(s);
  }

