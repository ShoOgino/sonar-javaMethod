  private static Set<Integer> argIndexes(List<String> params) {
    int index = 0;
    Set<Integer> result = Sets.newHashSet();
    for (String rawParam : params) {
      if (rawParam.contains("$")) {
        result.add(getIndex(rawParam));
      } else if (rawParam.charAt(0) != '<') {
        index++;
        result.add(index);
      }
    }
    return result;
  }

