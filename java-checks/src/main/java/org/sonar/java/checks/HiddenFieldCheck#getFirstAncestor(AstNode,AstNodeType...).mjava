  private static AstNode getFirstAncestor(AstNode node, AstNodeType... types) {
    for (AstNode ancestor = node.getParent(); ancestor != null; ancestor = ancestor.getParent()) {
      for (AstNodeType type : types) {
        if (ancestor.getType() == type) {
          return ancestor;
        }
      }
    }

    return null;
  }

