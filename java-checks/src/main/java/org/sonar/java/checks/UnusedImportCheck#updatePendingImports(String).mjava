  private void updatePendingImports(String reference) {
    if (!isFullyQualified(reference)) {
      Iterator<String> it = pendingImports.iterator();
      while (it.hasNext()) {
        String pendingImport = it.next();
        if (pendingImport.endsWith(reference)) {
          it.remove();
        }
      }
    }
  }

