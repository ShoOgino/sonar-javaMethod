  private static String getTypeIdentifierOrNull(Tree tree) {
    if (tree == null) {
      return null;
    }
    if (tree.is(Tree.Kind.PARAMETERIZED_TYPE)) {
      tree = ((ParameterizedTypeTree) tree).type();
    }
    if (tree.is(Tree.Kind.IDENTIFIER)) {
      return ((IdentifierTree) tree).name();
    }
    return null;
  }

