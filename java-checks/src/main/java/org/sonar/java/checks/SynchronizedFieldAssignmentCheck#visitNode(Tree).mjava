  @Override
  public void visitNode(Tree tree) {
    if (hasSemantic()) {
      SynchronizedStatementTree sst = (SynchronizedStatementTree) tree;
      Symbol field = getField(sst.expression());
      if (field != null) {
        sst.block().accept(new AssignmentVisitor(field, sst.expression()));
      }
    }
  }

