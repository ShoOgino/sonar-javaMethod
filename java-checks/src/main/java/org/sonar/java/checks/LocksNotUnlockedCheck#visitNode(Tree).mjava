  @Override
  public void visitNode(Tree tree) {
    if (!hasSemantic()) {
      return;
    }
    LockedVisitor visitor = new LockedVisitor();
    DataFlowVisitor.analyze((MethodTree) tree, visitor);
    for (Tree issueTree : visitor.getIssueTrees()) {
      addIssue(issueTree, "Unlock this lock.");
    }
  }

