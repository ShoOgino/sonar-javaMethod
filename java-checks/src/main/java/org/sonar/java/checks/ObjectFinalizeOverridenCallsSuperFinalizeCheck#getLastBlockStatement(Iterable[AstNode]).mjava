  private static AstNode getLastBlockStatement(Iterable<AstNode> blockStatements) {
    AstNode result = null;

    for (AstNode blockStatement : blockStatements) {
      AstNode statement = blockStatement.getFirstChild(JavaGrammar.STATEMENT);
      if (statement != null) {
        result = statement;
      }
    }

    return result;
  }

